(#let count 10)

(#procedure Int factorial ((Int num))
    (#when (#expr num <= 1)
        (return 1)
        )
    (return (mul num (factorial (sub num 1))))
    )

(#define foo bar)

(#let id (factorial count))

(#macro Void #unless {Bool cond, Obj body...}
    (return {#when (not $cond) $$body})
    )

(#procedure Int main {} {
    (#unless {#expr id < 40}
        (print_int id)
        )
    (return 0)
    })
