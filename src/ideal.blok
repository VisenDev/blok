(fn Int factorial (Int num)
    (let i 10)
    (if (num:lte 1)
        then 1
        else (factorial (num:sub 1))
        )
    )

(procedure Void test_for ()
    (for (Int i 0, i:lessthan 10, incr i)
        )
)

(procedure Int main (Int reference argc, Char[][] argv)
    (print_int (factorial 10))
    0
    )
